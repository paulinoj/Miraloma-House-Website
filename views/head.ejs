<script type="text/javascript">

$(function() {

	// blinking text
	var x;
	setInterval(function() {
			if(x == 0) {
				$('.blinking_text').removeAttr('style');
				x = 1;
			} else  {
				if(x = 1) {
					$('.blinking_text').css('color', 'black');
					x = 0;
				}
			}
	}, 1500); // change the time if you want
});
</script>


